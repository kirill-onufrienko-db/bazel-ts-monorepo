{
    "compilerOptions": {
        "baseUrl": "./",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "preserveConstEnums": true,
        "removeComments": true,
        "sourceMap": true,
        "outDir": "./dist",
        "target": "esnext",
        "module": "esnext",
        "paths": {
            "test_bazel_build/*": ["*"]
        }
    },
    "exclude": [
        "node_modules",
        "bazel-out",
        "dist",
        "**/*.spec.ts"
    ]
}
